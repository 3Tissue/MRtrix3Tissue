fod2fixel -nthread 1 fod.mif fod2fixel_tmp -afd afd.mif -peak peak.mif -disp disp.mif -force && testing_diff_fixel -frac 10e-5 fod2fixel/out1 fod2fixel_tmp && rm -rf fod2fixel_tmp

fod2fixel -nthread 1 fod.mif -mask mask.mif fod2fixel_tmp -afd afd.mif -peak peak.mif -disp disp.mif -force && testing_diff_fixel -frac 10e-5 fod2fixel/out2 fod2fixel_tmp && rm -rf fod2fixel_tmp

fod2fixel -nthread 1 -fmls_no_thresholds fod.mif fod2fixel_tmp -afd afd.mif -peak peak.mif -disp disp.mif -force && testing_diff_fixel -frac 10e-5 fod2fixel/out3 fod2fixel_tmp && rm -rf fod2fixel_tmp
